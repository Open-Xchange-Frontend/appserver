{
    "root": true,
    "extends": ["eslint:recommended"],
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
    	"no-nested-ternary": 0,
    	"no-empty": 0,
    	"no-new-func": 0,
        "no-multi-spaces": 0,
        "no-console": "off",
        "no-control-regex": "off"
    }
}
